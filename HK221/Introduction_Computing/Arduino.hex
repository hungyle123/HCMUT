void setup(){
  pinMode(9,OUTPUT);
  pinMode(10,OUTPUT);
}
void left_motor(int speed){
  if (speed > 0) {

  analogWrite(9,HIGH);
  digitalWrite(9,speed);
  digitalWrite(10,LOW);}
  else {
    speed = 0 - speed;
    digitalWrite(9,LOW);
    digitalWrite(10,HIGH);
  analogWrite(10,speed);
  }
}
void loop() {
  left_motor(200);
  delay(2000);
  left_motor(0);
  delay(2000);
  left_motor(-200);
  delay(2000);
}
